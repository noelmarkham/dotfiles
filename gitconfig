[user]
	name = Noel Markham
	email = noel@habito.com
[commit]
	template = ~/dev/noel/dotfiles/gitcommitmessage
[alias]
	statys = status
	np = log --oneline origin/master..master
	lg = log --color --graph --pretty=format:'%Cred%h%Creset - %C(bold blue)<%an>%Creset %C(yellow)%d%Creset %s %Cgreen(%cr)' --abbrev-commit
	files = diff-tree --no-commit-id --name-only -r
	dc = diff --cached
[core]
	excludesfile = ~/dev/noel/dotfiles/gitignore_global
[transfer] 
        fsckobjects = true 
[fetch] 
        fsckobjects = true 
[receive] 
        fsckObjects = true 